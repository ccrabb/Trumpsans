{
  "manifest_version": 2,
  "name": "Trumpsans",
  "description": "This extension applies a font far more suitable for President Trump's tweets",
  "version": "1.0",
  "permissions": [
    "https://twitter.com/",
    "http://twitter.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "http://twitter.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}